CREATE TABLE HECHO3 
    ( 
     EMPLEADO_ID NUMBER  NOT NULL , 
     MATERIAL_ID NUMBER  NOT NULL , 
     FECHA_I_ID NUMBER  NOT NULL , 
     FECHA_F_ID NUMBER  NOT NULL , 
     CANT_MATERIAL_UTILIZADO NUMBER , 
     CANT_OCUP_SG NUMBER ,
     TOTAL_OCUP_SG NUMBER , 
     CANT_TRABAJOS_SOBREPASAN NUMBER 
    ) 
;





ALTER TABLE HECHO3 
    ADD CONSTRAINT HECHO3_EMPLEADO_FK FOREIGN KEY 
    ( 
     EMPLEADO_ID
    ) 
    REFERENCES EMPLEADO 
    ( 
     EMPLEADO_ID
    ) 
;


ALTER TABLE HECHO3 
    ADD CONSTRAINT HECHO3_FECHA_FIN_FK FOREIGN KEY 
    ( 
     FECHA_F_ID
    ) 
    REFERENCES FECHA_FIN 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO3 
    ADD CONSTRAINT HECHO3_FECHA_INI_FK FOREIGN KEY 
    ( 
     FECHA_I_ID
    ) 
    REFERENCES FECHA_INI 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO3 
    ADD CONSTRAINT HECHO3_MATERIAL_FK FOREIGN KEY 
    ( 
     MATERIAL_ID
    ) 
    REFERENCES MATERIAL 
    ( 
     MATERIAL_ID
    ) 
;

