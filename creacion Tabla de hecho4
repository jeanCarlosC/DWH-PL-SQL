CREATE TABLE HECHO4 
    ( 
     FALLA_ID NUMBER  NOT NULL , 
     EDIFICIO_ID NUMBER  NOT NULL , 
     LOCALIDAD_ID NUMBER  NOT NULL , 
     FECHA_I_ID NUMBER  NOT NULL , 
     FECHA_F_ID NUMBER  NOT NULL , 
     CANT_FALLAS_SIMULT NUMBER , 
     TIEMP_FALLAS_IGUALES VARCHAR2 (10) , 
     TIEMP_FALLAS_DIF VARCHAR2 (30) , 
     CANT_TIPO_FALLAS_ASOC NUMBER , 
     CANT_FALLAS_COMUNES_EDIF NUMBER 
    ) 
;





ALTER TABLE HECHO4 
    ADD CONSTRAINT HECHO4_EDIFICIOS_FK FOREIGN KEY 
    ( 
     EDIFICIO_ID
    ) 
    REFERENCES EDIFICIOS 
    ( 
     EDIFICIO_ID
    ) 
;


ALTER TABLE HECHO4 
    ADD CONSTRAINT HECHO4_FALLA_FK FOREIGN KEY 
    ( 
     FALLA_ID
    ) 
    REFERENCES FALLA 
    ( 
     FALLA_ID
    ) 
;


ALTER TABLE HECHO4 
    ADD CONSTRAINT HECHO4_FECHA_FIN_FK FOREIGN KEY 
    ( 
     FECHA_F_ID
    ) 
    REFERENCES FECHA_FIN 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO4 
    ADD CONSTRAINT HECHO4_FECHA_INI_FK FOREIGN KEY 
    ( 
     FECHA_I_ID
    ) 
    REFERENCES FECHA_INI 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO4 
    ADD CONSTRAINT HECHO4_LOCALIDAD_FK FOREIGN KEY 
    ( 
     LOCALIDAD_ID
    ) 
    REFERENCES LOCALIDAD 
    ( 
     LOCALIDAD_ID
    ) 
;

