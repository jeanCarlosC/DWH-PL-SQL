CREATE TABLE HECHO2 
    ( 
     FALLA_ID NUMBER  NOT NULL , 
     RAMA_ID NUMBER  NOT NULL , 
     EMPLEADO_ID NUMBER  NOT NULL , 
     FECHA_I_ID NUMBER  NOT NULL , 
     FECHA_F_ID NUMBER  NOT NULL , 
     TIEM_PROM_EMP_FALLA FLOAT , 
     CANT_FALLA_EMP_RAMA NUMBER 
    ) 
;





ALTER TABLE HECHO2 
    ADD CONSTRAINT HECHO2_EMPLEADO_FK FOREIGN KEY 
    ( 
     EMPLEADO_ID
    ) 
    REFERENCES EMPLEADO 
    ( 
     EMPLEADO_ID
    ) 
;


ALTER TABLE HECHO2 
    ADD CONSTRAINT HECHO2_FALLA_FK FOREIGN KEY 
    ( 
     FALLA_ID
    ) 
    REFERENCES FALLA 
    ( 
     FALLA_ID
    ) 
;


ALTER TABLE HECHO2 
    ADD CONSTRAINT HECHO2_FECHA_FIN_FK FOREIGN KEY 
    ( 
     FECHA_F_ID
    ) 
    REFERENCES FECHA_FIN 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO2 
    ADD CONSTRAINT HECHO2_FECHA_INI_FK FOREIGN KEY 
    ( 
     FECHA_I_ID
    ) 
    REFERENCES FECHA_INI 
    ( 
     FECHA_ID
    ) 
;


ALTER TABLE HECHO2 
    ADD CONSTRAINT HECHO2_RAMA_FK FOREIGN KEY 
    ( 
     RAMA_ID
    ) 
    REFERENCES RAMA 
    ( 
     RAMA_ID
    ) 
;

